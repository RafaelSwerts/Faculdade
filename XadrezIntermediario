#include <stdio.h>

int main() {

    int torre =1, Bispo = 1,rainha, cavalo=0;

    do {
        printf(" Torre Direita \n");
        torre++;
        }
     while (torre<=5);


    while(Bispo != 6)  {
        printf(" Bispo Cima/Direita \n");
        Bispo++;
    }

    for(rainha=1; rainha <=8; rainha++) {
        printf(" Rainha Esquerda \n");
    }

while (cavalo < 1)

{
    for (int i = 0; i < 2; i++)
    {
        printf(" Cavalo Reto \n");
    }
      printf(" Cavalo lado \n");
    cavalo++;
}



    return 0;
    }
